mixin logo(className='')
  div(class=`logo ${className}`)
    picture
      source(
        srcset=`img/logo/logo.webp    1x,
                img/logo/logo@2x.webp 2x`
        type='image/webp'
      )
      img.logo__img(
        src='img/logo/logo.png'
        srcset='img/logo/logo@2x.png 2x'
        alt='«Cheelee» logo.'
        width='92'
        height='58'
      )
